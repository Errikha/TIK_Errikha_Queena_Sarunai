#include <iostream>
using namespace std;

int main(){
    //operator aritmatika
    
    int a = 10;
    int b = 5;
 
    
    int hasiltambah = a + b; //penjumlahan
    int hasilkurang = a - b; //pengurangan
    int hasilkali = a / b ; //perkalian
    int hasilbagi = a * b; //pembagian
    int sisabagi = a % b; //persenan
    
    cout<<hasiltambah<<endl;
    cout<<hasilkurang<<endl;
    cout<<hasilkali<<endl;
    cout<<hasilbagi<<endl;
    cout<<sisabagi<<endl;
   
    return 0;
}
